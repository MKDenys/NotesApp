<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/emptyFragment">
    <fragment
        android:id="@+id/emptyFragment"
        android:name="com.dk.notesapp.presentation.start.StartFragment"
        android:label="fragment_empty"
        tools:layout="@layout/fragment_start" >
        <action
            android:id="@+id/action_emptyFragment_to_notesListFragment"
            app:destination="@id/notesListFragment" />
    </fragment>
    <fragment
        android:id="@+id/notesListFragment"
        android:name="com.dk.notesapp.presentation.list.NotesListFragment"
        android:label="fragment_notes_list"
        tools:layout="@layout/fragment_notes_list" />
</navigation>